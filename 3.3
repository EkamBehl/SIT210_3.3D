int Led=D7;

void setup() {
    pinMode(Led,OUTPUT);
    Particle.subscribe("Deakin_RIOT_SIT210_Photon_Buddy",Handler);
    
    
}
void loop(){
    if(Particle.publish("Deakin_RIOT_SIT210_Photon_Buddy","pat")==true){
        digitalWrite(Led,HIGH);
        delay(1000);
        digitalWrite(Led,LOW);
        delay(1000);
        digitalWrite(Led,HIGH);
        delay(1000);
        digitalWrite(Led,LOW);
        delay(1000);
        digitalWrite(Led,HIGH);
        delay(1000);
        digitalWrite(Led,LOW);
        delay(1000);
    }
    else if(Particle.publish("Deakin_RIOT_SIT210_Photon_Buddy","wave")==true){
        digitalWrite(Led,HIGH);
        delay(100);
        digitalWrite(Led,LOW);
        delay(100);
        digitalWrite(Led,HIGH);
        delay(100);
        digitalWrite(Led,LOW);
        delay(100);
        digitalWrite(Led,HIGH);
        delay(100);
        digitalWrite(Led,LOW);
        delay(100);
        
    }
    delay(10000);
}
